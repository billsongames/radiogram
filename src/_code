      for (let i=0; i<response.length; i++) {
        let stationLogo
        let stationName

        if (response[i].favicon === "") {
          stationLogo = no_image
        } else {
          stationLogo = (response[i].favicon)
        }
        stationName = (response[i].name)

        info.push({favicon: stationLogo, name: stationName})
      }